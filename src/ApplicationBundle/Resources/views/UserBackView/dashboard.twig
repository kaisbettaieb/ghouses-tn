    {% include '@Application/PropBackView/header.html.twig' %}
</head>

<body>
<div class="wrapper">
    {{ include('@Application/UserBackView/sidebar.html.twig') }}
    <div class="main-panel">
        {{ include('@Application/UserBackView/navbar.html.twig') }}
        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-6">
                        <div class="card card-stats">
                            <div class="card-header" data-background-color="red">
                                <i class="material-icons">info_outline</i>
                            </div>
                            <div class="card-content">
                                <p class="category">Wish list</p>
                                <h3 class="title">75 Maisons</h3>
                            </div>
                            <div class="card-footer ">
                                <div class="stats">
                                    <i class="material-icons">local_offer</i> Les maisons que vous avez ajouté
                                </div>
                                <div class="pull-right"><a href="#">voir plus</a></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6">
                        <div class="card card-stats">
                            <div class="card-header" data-background-color="blue">
                                <i class="fa fa-twitter"></i>
                            </div>
                            <div class="card-content">
                                <p class="category">Commentaires</p>
                                <h3 class="title">+20</h3>
                            </div>
                            <div class="card-footer">
                                <div class="stats">
                                    <i class="material-icons">update</i> Vos commentaires
                                </div>
